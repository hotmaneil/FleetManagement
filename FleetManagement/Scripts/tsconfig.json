{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true, //編譯時刪除註釋
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
   "compileOnSave": true,//儲存時重新編譯
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
